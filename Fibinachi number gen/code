#fibinachi sequence gen
import time
stop = input("How far will you go?: ")
stop = int(stop)
number1 = 0
number2 = 1
sequence_num = 0
while True:
    sequence_num += 1
    result = number1 + number2
    number1 = number2
    number2 = result
    #print("sequence number: " +str(sequence_num) + ". Fibinachi number: " + str(result))#, end = "\r")
    if sequence_num == stop:
        print("sequence number: " +str(sequence_num) + ". Fibinachi number: " + str(result))#, end = "\r")
        print("")
        break
